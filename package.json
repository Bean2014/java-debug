{
  "name": "java-debug",
  "displayName": "Java Debug",
  "description": "A simple debug extension for the Java",
  "version": "0.0.2",
  "publisher": "DSnake",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#38f",
    "theme": "dark"
  },
  "license": "Apache License 2.0",
  "bugs": {
    "url": "https://github.com/BinZone/java-debug/issues"
  },
  "homepage": "http://binzone.github.io/java-debug/",
  "repository": {
    "type": "git",
    "url": "https://github.com/BinZone/java-debug.git"
  },
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": [
    "Languages",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:java",
    "onCommand:javaDebug.compile",
    "onCommand:javaDebug.run"
  ],
  "main": "./out/javaDebug",
  "contributes": {
    "commands": [{
        "command": "javaDebug.compile",
        "title": "Java: compile"
      },
      {
        "command": "javaDebug.run",
        "title": "Java: run"
      }
    ],
    "keybindings": [{
        "command": "javaDebug.compile",
        "key": "alt+c",
        "mac": "alt+c",
        "when": "editorTextFocus"
      },
      {
        "command": "javaDebug.run",
        "key": "alt+r",
        "mac": "alt+r",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "tslint": "^5.8.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}